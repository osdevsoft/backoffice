{
  "name": "osds/backoffice",
  "description": "Easy backoffice for Frameworks",
  "authors": [
    {
      "name": "Open Source Development Services",
      "email": "info@osds.com"
    }
  ],
  "minimum-stability": "dev",
  "version": "1.0a",
  "require": {
    "phpunit/phpunit": "^7.0",
    "guzzlehttp/guzzle": "6.3.*",
    "symfony/yaml": "4.2.*",
    "doctrine/annotations": "1.6.*",
    "osds/ddd-common": "dev-master",
    "osds/auth": "dev-master",
    "osds/template-blocks": "dev-master"
  },

  "repositories": {
    "osds-ddd-common": {
      "type": "vcs",
      "url": "https://github.com/osdevsoft/ddd-common"
    },
    "osds-auth": {
      "type": "vcs",
      "url": "https://github.com/osdevsoft/auth"
    },
    "template-blocks": {
      "type": "vcs",
      "url": "https://github.com/osdevsoft/template-blocks"
    }
  },

  "autoload": {
    "psr-4": {
      "Osds\\Backoffice\\": "src/"
    },
    "files": [
      "src/utils.php"
    ]
  }
}
